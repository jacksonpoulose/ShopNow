    <script>
        document.addEventListener('DOMContentLoaded',function (){
            const addToCartForms = document.querySelectorAll(".btn-product btn-wishlist");
    
            addToCartForms.forEach(form => {
          form.addEventListener("submit", async (event) => {
            event.preventDefault(); // Prevent the default form submission
    
            const productId = form.action.split("/").pop(); // Extract product ID from action URL
    
            try {
              // Send a POST request to the server
              const response = await fetch(`/user/add-to-wishlist/${productId}`, {
                method: "POST",
                headers: {
                  "Content-Type": "application/json"
                },
                body: JSON.stringify({ productId })
              });
    
              // Handle the response
              if (response.ok) {
                const data = await response.json();
                alert(data.message || "Product added to wishlist successfully!");
               
              } else {
                const error = await response.json();
                alert(error.message || "Failed to add product to wishlist.");
              }
            } catch (err) {
              console.error("Error adding to wishlist:", err);
              alert("An error occurred. Please try again later.");
            }
          });
        });
        })
       </script>


   <script>
    document.addEventListener('DOMContentLoaded',function (){
        const addToCartForms = document.querySelectorAll(".btn-product btn-cart");

        addToCartForms.forEach(form => {
      form.addEventListener("submit", async (event) => {
        event.preventDefault(); // Prevent the default form submission

        const productId = form.action.split("/").pop(); // Extract product ID from action URL

        try {
          // Send a POST request to the server
          const response = await fetch(`/user/add-to-cart/${productId}`, {
            method: "POST",
            headers: {
              "Content-Type": "application/json"
            },
            body: JSON.stringify({ productId })
          });

          // Handle the response
          if (response.ok) {
            const data = await response.json();
            alert(data.message || "Product added to cart successfully!");
           
          } else {
            const error = await response.json();
            alert(error.message || "Failed to add product to cart.");
          }
        } catch (err) {
          console.error("Error adding to cart:", err);
          alert("An error occurred. Please try again later.");
        }
      });
    });
    })
   </script>
   
